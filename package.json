  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "date-and-time": "^0.6.3",
    "exceljs": "^4.1.1",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "googleapis": "^39.2.0",
    "https": "^1.0.0",
    "mongoose": "^5.4.3",
    "mysql": "^2.16.0",
    "node-cron": "^2.0.3",
    "puppeteer": "^1.11.0",
    "puppeteer-firefox": "^0.4.2",
    "querystring": "^0.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
